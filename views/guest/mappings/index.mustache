<html>
  <head>
    {{>templates/header}}
  </head>
  <body>
    <div class="wrapper">
      <div class="container">
        {{>templates/navbar}}

        <table class="table table-hover table-striped">
          <thead>
            <tr>
              <th> Type </th>
              <th> Mapping </th>
            </tr>
          </thead>
          <tbody>
            {{#mappings}}
              <tr>
                <td> {{type}} </td>
                <td id="code"> {{mapping}} </td>
              </tr>
            {{/mappings}}
          </tbody>
        </table>

      </div>
    </div>

    var aA=document.querySelectorAll("#code");
    for(var i=0;i<aA.length;i++){
      aA[i].textContent = aA[i].textContent.replace(/%0A/g,"\r\n").replace(/&amp;/g, "&").replace(/&gt/g, ">;").replace(/&lt/g, "<;").replace(/&quot;/g, '"').replace(/&#x2F;/g,"/").replace(/&#39;/g,"'");
    }
  </body>
</html>
